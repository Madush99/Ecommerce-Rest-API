    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "react-paypal-button-v2": "^2.6.3",
    "react-redux": "^7.2.4",
    "redux-devtools-extension": "^2.13.9",
